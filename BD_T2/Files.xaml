        <Button x:Name="Correta" Content="Correta" HorizontalAlignment="Left" Height="46" Margin="10,10,0,0" VerticalAlignment="Top" Width="107" Click="Correta_Click"/>
        <Button x:Name="Lock" Content="Lock" HorizontalAlignment="Left" Height="46" Margin="10,61,0,0" VerticalAlignment="Top" Width="107" Click="Lock_Click"/>
        <Button x:Name="Deadlock" Content="Deadlock" HorizontalAlignment="Left" Height="46" Margin="122,10,0,0" VerticalAlignment="Top" Width="107" Click="Deadlock_Click"/>
